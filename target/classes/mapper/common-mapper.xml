<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="common">
	<select id='interestSel' parameterType='_int' resultType='map'>
		select * from DETAILINTEREST where INTERESTNO=#{interestNo}
	</select>
	<insert id='messageSend' parameterType='map'>
		insert into message values(seq_messageNo.nextval,#{messageContent},sysdate,null,default,default,#{receiveId},#{sendId})
	</insert>
</mapper>
